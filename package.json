{
  "name": "playbook-cli",
  "version": "1.3.3",
  "description": "Keep track of your favorite commands.",
  "main": "src/index.ts",
  "bin": {
    "playbook": "dist/index.js"
  },
  "preferGlobal": true,
  "scripts": {
    "bundle": "webpack",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build2": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "test": "jest",
    "lint": "eslint .",
    "build3": "pkg ./package.json --out-path ./dist || echo '\nPlease first `npm install -g pkg` to run this command'",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "watch": "tsc --watch -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mabarbeau/projectman.git"
  },
  "keywords": [
    "cli",
    "open-projects",
    "configurable-projects"
  ],
  "authors": [
    "Marc Barbeau <hello@marcbarbeau.com> (https://marcbarbeau.com)",
    "Saurabh Daware <saurabhdaware99@gmail.com> (https://saurabhdaware.in)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mabarbeau/projectman/issues"
  },
  "homepage": "https://github.com/mabarbeau/projectman#readme",
  "dependencies": {
    "@types/jest": "^26.0.0",
    "commander": "^3.0.2",
    "figlet": "^1.4.0",
    "prompts": "^2.3.2",
    "string": "^3.3.3",
    "terminal-kit": "^1.35.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@types/figlet": "^1.2.0",
    "@types/node": "^14.0.13",
    "@types/prompts": "^2.0.8",
    "@types/string": "0.0.30",
    "@types/terminal-kit": "^1.28.2",
    "babel-loader": "^8.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
